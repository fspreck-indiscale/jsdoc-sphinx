{
  "name": "@indiscale/jsdoc-sphinx",
  "version": "0.0.7",
  "description": "A JSDoc3 template which output ReStructured Text for Sphinx Doc",
  "main": "/template/publish.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fspreck-indiscale/jsdoc-sphinx.git"
  },
  "keywords": [
    "jsdoc",
    "jsdoc3",
    "template",
    "rst",
    "restructuredtext",
    "sphinx",
    "javascript",
    "documentation",
    "generator"
  ],
  "author": "Florian Spreckelsen <f.spreckelsen@indiscale.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fspreck-indiscale/jsdoc-sphinx/issues"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "valid-jsdoc": 2,
      "semi": 2
    }
  },
  "homepage": "https://github.com/fspreck-indiscale/jsdoc-sphinx",
  "peerDependencies": {
    "jsdoc": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^2",
    "eslint-config-google": "^0.5.0",
    "jsdoc": "^4.0.0"
  },
  "dependencies": {
    "async": "^1.5.0",
    "canonical-path": "0.0.2",
    "deasync": "^0.1.3",
    "lodash": "^3.10.1",
    "mkdirp": "^0.5.1",
    "mustache": "^2.2.0"
  }
}
